{
  "name": "api",
  "version": "0.1.0",
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && bun install",
    "dev": "NODE_ENV=development bun run ./src",
    "dev:hot": "bun --watch ./src",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint --fix",
    "start": "NODE_ENV=production bun run -r ./config.js ./dist",
    "test": "NODE_ENV=test bun run ./spec",
    "test:hot": "bun --watch ./spec --watch ./src",
    "type-check": "bunx tsc --noEmit"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin-js": "^2.13.0",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "dotenv": "^16.4.7",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-n": "^17.15.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.5",
    "jasmine": "^5.5.0",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-schema": "^1.4.3",
    "jet-validators": "^1.0.16",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "supertest": "^7.0.0",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "jiti": "^2.4.2",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.5",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0"
  }
}
